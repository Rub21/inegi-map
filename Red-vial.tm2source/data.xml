<!DOCTYPE Map[]>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">

<Parameters>
  <Parameter name="center">-90.1566,20.5878,9</Parameter>
  <Parameter name="format">pbf</Parameter>
  <Parameter name="json"><![CDATA[{"vector_layers":[{"id":"red_vial_nad","description":"","fields":{"gid":"Number","nombre":"String"}},{"id":"Red_Vial-NAD83","description":"","fields":{"TIPO_VIAL":"String","NOMBRE":"String","CARRILES":"String","VELOCIDAD":"String"}}]}]]></Parameter>
  <Parameter name="maxzoom">17</Parameter>
  <Parameter name="minzoom">0</Parameter>
  <Parameter name="name"><![CDATA[Red_vial]]></Parameter>
</Parameters>


<Layer name="red_vial_nad"
  buffer-size="8"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    
    <Datasource>
       <Parameter name="dbname"><![CDATA[dbinegi]]></Parameter>
       <Parameter name="extent"><![CDATA[-180,-85.0511,180,85.0511]]></Parameter>
       <Parameter name="geometry_field"><![CDATA[geom]]></Parameter>
       <Parameter name="geometry_table"><![CDATA[]]></Parameter>
       <Parameter name="host"><![CDATA[localhost]]></Parameter>
       <Parameter name="key_field"><![CDATA[gid]]></Parameter>
       <Parameter name="max_size"><![CDATA[512]]></Parameter>
       <Parameter name="password"><![CDATA[1234]]></Parameter>
       <Parameter name="port"><![CDATA[5432]]></Parameter>
       <Parameter name="table"><![CDATA[  ( SELECT gid,nombre,geom FROM red_vial_nad
         WHERE geom && !bbox! AND CASE
         	WHEN z(!scale_denominator!)>=6 THEN TRUE
         	WHEN z(!scale_denominator!)>=7 THEN TRUE
         	WHEN z(!scale_denominator!)>=8 THEN TRUE
          	WHEN z(!scale_denominator!)>=9 THEN TRUE
         	WHEN z(!scale_denominator!)>=11 THEN TRUE
          	WHEN z(!scale_denominator!)>=12 THEN TRUE
          	WHEN z(!scale_denominator!)>=13 THEN TRUE
         END
  ) AS data]]></Parameter>
       <Parameter name="type"><![CDATA[postgis]]></Parameter>
       <Parameter name="user"><![CDATA[postgres]]></Parameter>
    </Datasource>
  </Layer>

